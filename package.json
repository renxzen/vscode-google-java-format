{
  "name": "google-java-format",
  "displayName": "google-java-format",
  "description": "Yet another simple google-java-format runner",
  "publisher": "serikb",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/serikb/vscode-google-java-format"
  },
  "keywords": [
    "Java"
  ],
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Google-java-format configuration",
      "properties": {
        "google-java-format.executable-path": {
          "type": "string",
          "default": "/usr/local/bin/google-java-format",
          "description": "Path to google-java-format script"
        },
        "google-java-format.extra-args": {
          "type": "string",
          "default": "",
          "description": "Any extra arguments to pass to google-java-format script"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
