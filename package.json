{
	"name": "google-java-format",
	"displayName": "google-java-format",
	"description": "Simple google-java-format runner with optional tab conversion",
	"publisher": "renxzen",
	"version": "1.1.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/renxzen/vscode-google-java-format"
	},
	"keywords": [
		"google",
		"java",
		"format",
		"google-java-format",
		"renxzen"
	],
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:java"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Google-java-format configuration",
			"properties": {
				"google-java-format.args": {
					"type": "string",
					"default": "",
					"description": "Any extra arguments to pass to google-java-format script"
				},
				"google-java-format.aosp-mode": {
					"type": "boolean",
					"default": false,
					"description": "Use the aosp mode, with four spaces indentation and imports separated by newlines"
				},
				"google-java-format.use-tabs": {
					"type": "boolean",
					"default": false,
					"description": "Replace spaces with tabs"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"vscode-test": "^1.4.0"
	}
}